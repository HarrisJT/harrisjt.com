{
  "name": "harrisjt",
  "description": "Software Development, Design, and more, by HarrisJT",
  "version": "0.0.1",
  "private": true,
  "author": "Harris Thompson <harris@harrisjt.com>",
  "homepage": "https://harrisjt.com",
  "keywords": [
    "harrisjt",
    "hjt",
    "portfolio",
    "blog",
    "development",
    "design",
    "software",
    "gatsby"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/harrisjt/harrisjt.com"
  },
  "bugs": {
    "url": "https://github.com/harrisjt/harrisjt.com/issues"
  },
  "scripts": {
    "build": "npm run purge && gatsby build --verbose",
    "deploy": "npm run format && npm run build && netlifyctl deploy",
    "dev": "gatsby develop",
    "format": "npm-run-all -p format:*",
    "format:js": "prettier-eslint --config .prettierrc --write \"**/*.{js,jsx}\"",
    "format:markdown": "prettier --parser markdown --write \"**/*.md\"",
    "hook:install": "precommit install",
    "hook:remove": "precommit remove",
    "purge": "del-cli public/ && del-cli .cache/",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "pre-commit": [
    "purge",
    "format"
  ],
  "resolutions": {
    "gatsby/graphql": "0.10.5",
    "gatsby/react": "16.0.0",
    "gatsby/react-dom": "16.0.0"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.10.0",
    "gatsby": "2.0.0",
    "gatsby-link": "2.2.0",
    "gatsby-plugin-catch-links": "2.1.0",
    "gatsby-plugin-favicon": "2.1.1",
    "gatsby-plugin-feed": "2.3.1",
    "gatsby-plugin-google-analytics": "2.1.1",
    "gatsby-plugin-manifest": "2.2.1",
    "gatsby-plugin-netlify": "2.1.0",
    "gatsby-plugin-offline": "2.2.0",
    "gatsby-plugin-react-helmet": "3.1.0",
    "gatsby-plugin-react-next": "1.0.11",
    "gatsby-plugin-sharp": "2.0.31",
    "gatsby-plugin-sitemap": "2.2.1",
    "gatsby-plugin-styled-components": "3.1.0",
    "gatsby-remark-copy-linked-files": "2.1.0",
    "gatsby-remark-images": "3.1.2",
    "gatsby-remark-prismjs": "3.3.1",
    "gatsby-remark-smartypants": "2.1.0",
    "gatsby-source-filesystem": "2.0.22",
    "gatsby-transformer-remark": "2.5.0",
    "gatsby-transformer-sharp": "2.2.1",
    "prismjs": "1.16.0",
    "prop-types": "15.7.2",
    "react-helmet": "5.2.1",
    "styled-components": "3.4.10",
    "typeface-asul": "0.0.69",
    "typeface-open-sans": "0.0.54",
    "snyk": "^1.170.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "del-cli": "2.0.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "netlify": "2.4.4",
    "npm-run-all": "4.1.5",
    "pre-commit": "1.2.2",
    "prettier": "1.17.1",
    "prettier-eslint-cli": "4.7.1"
  },
  "license": "MIT",
  "snyk": true
}
