    "develop": "gatsby develop",
    "lint": "eslint --cache --fix \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"**/*.{css,html,js,jsx,json,md,scss,ts,tsx,yaml,yml}\"",
