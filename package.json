{
  "name": "harrisjt",
  "description": "Software Development, Design, and more, by HarrisJT",
  "version": "0.0.1",
  "private": true,
  "author": "Harris Thompson <harris@harrisjt.com>",
  "homepage": "https://harrisjt.com",
  "keywords": [
    "harrisjt",
    "hjt",
    "portfolio",
    "blog",
    "development",
    "design",
    "software",
    "gatsby"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/harrisjt/harrisjt.com"
  },
  "bugs": {
    "url": "https://github.com/harrisjt/harrisjt.com/issues"
  },
  "scripts": {
    "build": "npm run purge && gatsby build --verbose",
    "deploy": "npm run format && npm run build && netlifyctl deploy",
    "dev": "gatsby develop",
    "format": "npm-run-all -p format:*",
    "format:js": "prettier-eslint --config .prettierrc --write \"**/*.{js,jsx}\"",
    "format:markdown": "prettier --parser markdown --write \"**/*.md\"",
    "hook:install": "precommit install",
    "hook:remove": "precommit remove",
    "purge": "del-cli public/ && del-cli .cache/"
  },
  "pre-commit": [
    "purge",
    "format"
  ],
  "resolutions": {
    "gatsby/graphql": "0.10.5",
    "gatsby/react": "16.0.0",
    "gatsby/react-dom": "16.0.0"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.5.1",
    "gatsby": "1.9.260",
    "gatsby-link": "1.6.44",
    "gatsby-plugin-catch-links": "1.0.22",
    "gatsby-plugin-favicon": "2.1.1",
    "gatsby-plugin-feed": "1.3.22",
    "gatsby-plugin-google-analytics": "1.0.31",
    "gatsby-plugin-manifest": "1.0.21",
    "gatsby-plugin-netlify": "1.0.19",
    "gatsby-plugin-offline": "1.0.16",
    "gatsby-plugin-react-helmet": "2.0.11",
    "gatsby-plugin-react-next": "1.0.11",
    "gatsby-plugin-sharp": "1.6.44",
    "gatsby-plugin-sitemap": "1.2.23",
    "gatsby-plugin-styled-components": "2.0.11",
    "gatsby-remark-copy-linked-files": "1.5.32",
    "gatsby-remark-images": "1.5.63",
    "gatsby-remark-prismjs": "2.0.2",
    "gatsby-remark-smartypants": "1.4.12",
    "gatsby-source-filesystem": "1.5.35",
    "gatsby-transformer-remark": "1.7.41",
    "gatsby-transformer-sharp": "1.6.24",
    "prismjs": "1.14.0",
    "prop-types": "15.6.1",
    "react-helmet": "5.2.0",
    "styled-components": "3.2.6",
    "typeface-asul": "0.0.54",
    "typeface-open-sans": "0.0.54"
  },
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "del-cli": "1.1.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.8.2",
    "netlify": "1.2.0",
    "npm-run-all": "4.1.3",
    "pre-commit": "1.2.2",
    "prettier": "1.12.1",
    "prettier-eslint-cli": "4.7.1"
  },
  "license": "MIT"
}
